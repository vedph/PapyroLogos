<div n="1" subtype="column" type="textpart" corresp="#FR6239" part="N" org="uniform" sample="complete"><lg n="1" part="N" org="uniform" sample="complete"><l part="N">
<lb n="0"/><orig>εμφαε</orig></l></lg>
    <lg n="2" part="N" org="uniform" sample="complete">
        <l n="19" part="N">
            <lb n="1"/>ἤδ<supplied reason="lost" instant="false">η</supplied> μὲν ἀρτιφανὴς</l>
        <l n="20" part="N">Ἕω<supplied reason="lost" instant="false">ς ἱππεύει</supplied> <supplied evidence="parallel" reason="lost" instant="false">κατὰ γᾶν</supplied>·</l>
        <l n="21" part="N">
            <lb n="2"/>ὑπὲρ δ’ ἐμᾶς κεφαλᾶς</l>
        <l n="22" part="N">Πλειὰ<supplied reason="lost" instant="false">ς ἐκλείπει σκοτία</supplied>·</l>
        <l n="23" part="N">
            <lb n="3"/>μέλπει <add place="above" instant="false" status="unremarkable">δὲ</add> δένδρεσι λεπτὰν</l>
        <l n="24" part="N">ἀηδὼν ἁρ<unclear instant="false">μ</unclear><supplied evidence="parallel" reason="lost" instant="false">ονίαν</supplied></l>
        <l n="25" part="N"><supplied evidence="parallel" reason="lost" instant="false">ὀρθρευ</supplied>
            <lb n="4" break="no"/>ομένα γόοις</l>
        <l n="26" part="N">Ἴτυν Ἴτυν πολύθρηνον.</l>
    </lg>
    <lg n="3" part="N" org="uniform" sample="complete">
        <l n="27" part="N"><supplied evidence="parallel" reason="lost" instant="false">σύριγγας δ’</supplied>
            <lb n="5"/>ο<add place="above" instant="false" status="unremarkable">ὐ</add>ριβάται</l>
        <l n="28" part="N">κινοῦσιν ποιμνᾶν ἐλάται·</l>
        <l n="29" part="N"><supplied evidence="parallel" reason="lost" instant="false">ἔγρονται</supplied>
            <lb n="6"/>δ’ εἰς βοτάναν</l>
        <l n="30" part="N">ξανθᾶν <supplied evidence="parallel" reason="lost" instant="false">πώλ</supplied>ων συζ<supplied evidence="parallel" reason="lost" instant="false">υγίαι</supplied>·</l>
        <l n="31" part="N"><supplied evidence="parallel" reason="lost" instant="false">ἤδη δ’ <g type="coronis"/>
            <lb n="7"/>εἰς ἔργα</supplied> κ<supplied evidence="parallel" reason="lost" instant="false">υναγ</supplied>οὶ</l>
        <l n="32" part="N"><choice><corr instant="false"><supplied evidence="parallel" reason="lost" instant="false">στ</supplied>είχουσι<supplied evidence="parallel" reason="lost" instant="false">ν</supplied> <unclear instant="false">θ</unclear><supplied evidence="parallel" reason="lost" instant="false">ηροφόνοι</supplied></corr><sic> <unclear instant="false">θ</unclear><supplied evidence="parallel" reason="lost" instant="false">ηροφόνοι στ</supplied>είχουσι<supplied evidence="parallel" reason="lost" instant="false">ν</supplied></sic></choice>.</l>
        <l n="33" part="N"><supplied evidence="parallel" reason="lost" instant="false">πηγαῖς δ’ ἐπ’
            <lb n="8"/>Ὠκεανοῦ</supplied></l>
        <l n="34" part="N">μελιβόα<supplied evidence="parallel" reason="lost" instant="false">ς κύκνος ἀχεῖ.</supplied></l>
    </lg>
    <lg n="4" part="N" org="uniform" sample="complete">
        <l n="35" part="N"><supplied evidence="parallel" reason="lost" instant="false">ἄκατοι δ’ ἀνάγον</supplied>
            <lb n="9" break="no"/>ται ὑπ’ εἰρεσία<surplus instant="false">ι</surplus>ς</l>
        <l n="36" part="N"><app type="editorial"><lem resp="BKT V.2"><supplied evidence="parallel" reason="lost" instant="false">ἀνέμων τ’ εὐάεσσιν ῥοθίοις</supplied></lem><rdg resp=" Schubart"><supplied reason="lost" instant="false">ἀνέ</supplied><unclear instant="false">μω</unclear><supplied reason="lost" instant="false">ν ῥοθίοις</supplied></rdg><rdg resp="Wilamowitz"><supplied reason="lost" instant="false">ἐυ</supplied><unclear instant="false">άτ</unclear><supplied reason="lost" instant="false">εσσιν ῥοθίοις</supplied></rdg></app>.</l>
        <l n="37" part="N">ἀν<supplied evidence="parallel" reason="lost" instant="false">ὰ δ’ ἱστία</supplied> <supplied reason="lost" instant="false">ναῦται</supplied>
            <lb n="10"/>ἀειράμενοι</l>
        <l n="38" part="N">ἀχ<surplus instant="false">ε</surplus>οῦσιν <supplied reason="lost" instant="false">ἄγου πό</supplied>τνι’ αὔρ<supplied reason="lost" instant="false">α</supplied></l>
        <l n="39" part="N"><supplied reason="lost" instant="false">ἡμᾶς σὺν</supplied><milestone rend="paragraphos" unit="undefined"/>
            <lb n="11"/>ἀκύμονι πομπᾶι</l>
        <l n="40" part="N">σιγώντων ἀνέμων</l>
        <l n="41" part="N"><supplied reason="lost" instant="false">ποτὶ τέκνα</supplied>
            <lb n="12"/>τε καὶ φιλίας ἀλόχους·</l>
        <l n="42" part="N">σινδὼν δὲ π<supplied evidence="parallel" reason="lost" instant="false">ρότονον ἐπὶ μέ</supplied><milestone rend="paragraphos" unit="undefined"/>
            <lb n="13" break="no"/>σομ πελάζει.</l>
    </lg>
    <lg n="5" part="N" org="uniform" sample="complete">
        <l n="43" part="N">τὰ μ<unclear instant="false">ὲ</unclear>ν οὖν <surplus instant="false">ἑτέρων</surplus> ἑτέρ<supplied evidence="parallel" reason="lost" instant="false">οισι μέριμνα πέλει</supplied>·</l>
        <l n="44" part="N"><milestone rend="paragraphos" unit="undefined"/>
            <lb n="14"/>κόσμ<choice><corr instant="false">ο</corr><sic>ει</sic></choice>ν <supplied reason="omitted" instant="false">δ</supplied>’ ὑμεναί<supplied reason="lost" instant="false">ω</supplied>ν <surplus instant="false">δὲ ἀεὶ</surplus> δεσποσύνω<supplied evidence="parallel" reason="lost" instant="false">ν</supplied></l>
        <l n="45" part="N"><supplied evidence="parallel" reason="lost" instant="false">ἐμὲ καὶ τὸ
            <lb n="15"/>δίκαι</supplied>ον ἄγει καὶ ἔρ<supplied evidence="parallel" reason="lost" instant="false">ως</supplied></l> 
        <l n="46" part="N"><supplied evidence="parallel" reason="lost" instant="false">ὑμ</supplied>νεῖν· δμωσὶ<supplied evidence="parallel" reason="lost" instant="false">ν γὰρ ἀνάκ
            <lb n="16" break="no"/>των</supplied></l>
        <l n="47" part="N"><supplied evidence="parallel" reason="lost" instant="false">εὐ</supplied><choice><reg instant="false">α</reg><orig>η</orig></choice>μερ<choice><corr instant="false">ίαι</corr><sic>οι</sic></choice> προσι<supplied reason="lost" instant="false">ο</supplied>ῦσαι</l>
        <l n="48" part="N">μολπᾶι θάρσος <supplied evidence="parallel" reason="lost" instant="false">ἄγουσ’</supplied></l>
        <l n="49" part="N"><lb n="17"/><supplied evidence="parallel" reason="lost" instant="false">ἐπιχάρμα</supplied>τά τ<surplus instant="false">ε</surplus>’· εἰ δὲ τύχα τι τέκ<choice><corr instant="false">οι</corr><sic>ηι</sic></choice>,</l>
        <l n="50" part="N">βα<supplied evidence="parallel" reason="lost" instant="false">ρὺν βαρεῖα
            <lb n="18"/>φόβον</supplied> ἔπεμψεν οἴκοις.</l>
    </lg>
    <lg n="6" part="N" org="uniform" sample="complete">
        <l n="51" part="N">ὁρίζεται δὲ τόδ<supplied evidence="parallel" reason="lost" instant="false">ε φάος
            <lb n="19"/>γάμω</supplied>ν τέλει,</l>
        <l n="52" part="N">τὸ δ<supplied evidence="parallel" reason="lost" instant="false">ή</supplied> ποτ’ εὐχαῖ<supplied evidence="parallel" reason="lost" instant="false">ς ἐγὼ</supplied></l>
        <l n="53" part="N">λισσο<supplied evidence="parallel" reason="lost" instant="false">μένα προσέβαν</supplied></l>
    </lg> 
