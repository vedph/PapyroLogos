<lb n="1"/><supplied reason="lost" instant="false">τῷ δʼ ἤδη δύ</supplied><unclear instant="false">ο</unclear> <unclear instant="false">μ</unclear><supplied reason="lost" instant="false">ὲν γενεαὶ μερόπων ἀνθρώπων</supplied>
<lb n="2"/><unclear instant="false">ἐ</unclear>φθί<unclear instant="false">αθ</unclear>ʼ, οἵ οἱ πρόσ<supplied reason="lost" instant="false">θεν ἅμα τράφεν ἠδʼ ἐγένοντο</supplied>
<lb n="3"/><unclear instant="false">ἐ</unclear>ν Πύλῳ ἠγαθέ<supplied reason="lost" instant="false">ῃ, μετὰ δὲ τριτάτοισιν ἄνασσεν·</supplied>
<lb n="4"/><unclear instant="false">ὅ</unclear> σφιν ἐὺ φρον<unclear instant="false">έ</unclear><supplied reason="lost" instant="false">ων ἀγορήσατο καὶ μετέειπεν·</supplied>
<lb n="5"/>ὦ πόποι ἦ μέγ<unclear instant="false">α</unclear> <supplied reason="lost" instant="false">πένθος Ἀχαιΐδα γαῖαν ἱκάνει·</supplied>
<lb n="6"/>ἦ κεν γ<unclear instant="false">η</unclear>θήσαι Π<supplied reason="lost" instant="false">ρίαμος Πριάμοιό τε παῖδες</supplied>
<lb n="7"/><supplied reason="lost" instant="false">ἄ</supplied>λλοι τ<unclear instant="false">ε</unclear> Τρῶες <supplied reason="lost" instant="false">μέγα κεν κεχαροίατο θυμῷ</supplied>
<lb n="8"/><supplied reason="lost" instant="false">εἰ</supplied> <unclear instant="false">σ</unclear>φῶϊν τά<unclear instant="false">δ</unclear>ε <supplied reason="lost" instant="false">πάντα πυθοίατο μαρναμένοιϊν,</supplied>
<lb n="9"/>ο<unclear instant="false">ἳ</unclear> πε<unclear instant="false">ρὶ</unclear> <unclear instant="false">μ</unclear>ὲν <unclear instant="false">β</unclear>ου<unclear instant="false">λ</unclear><supplied reason="lost" instant="false">ὴν Δαναῶν, περὶ δʼ ἐστὲ μάχεσθαι.</supplied>
<lb n="10"/><unclear instant="false">ἀλ</unclear>λὰ π<unclear instant="false">ίθ</unclear>ε<unclear instant="false">σθ</unclear>ʼ· ἄ<unclear instant="false">μ</unclear><supplied reason="lost" instant="false">φω δὲ νεωτέρω ἐστὸν ἐμεῖο·</supplied>
<lb n="11"/>ἤδη γ<unclear instant="false">ά</unclear><supplied reason="lost" instant="false">ρ</supplied> ποτʼ ἐγ<supplied reason="lost" instant="false">ὼ καὶ ἀρείοσιν ἠέ περ ὑμῖν</supplied>
<lb n="12"/>ἀνδρά<unclear instant="false">σιν</unclear> ὡ<unclear instant="false">μ</unclear><supplied reason="lost" instant="false">ίλησα, καὶ οὔ ποτέ μʼ οἵ γʼ ἀθέριζον.</supplied>
<lb n="13"/>οὐ γάρ π<unclear instant="false">ω</unclear> <unclear instant="false">τ</unclear>οίους <unclear instant="false">ἴδ</unclear><supplied reason="lost" instant="false">ον ἀνέρας οὐδὲ ἴδωμαι,</supplied>
<lb n="14" xml:id="div2-lb15"/><unclear instant="false">ο</unclear>ἷον Π<unclear instant="false">ε</unclear><supplied reason="lost" instant="false">ι</supplied><unclear instant="false">ρ</unclear>ίθοόν <supplied reason="lost" instant="false">τε Δρύαντά τε ποιμένα λαῶν</supplied>
<lb n="15" xml:id="div2-lb16"/>Κ<unclear instant="false">α</unclear>ινέ<unclear instant="false">α</unclear> τʼ Ἐξά<supplied reason="lost" instant="false">διόν τε καὶ ἀντίθεον Πολύφημον</supplied>
<lb n="16"/><unclear instant="false">κά</unclear>ρ<unclear instant="false">τιστ</unclear>ο<unclear instant="false">ι</unclear> <unclear instant="false">δὴ</unclear> <supplied reason="lost" instant="false">κεῖνοι ἐπιχθονίων τράφεν ἀνδρῶν·</supplied>
